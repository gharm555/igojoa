<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itwill.igojoa.repository.UserDao">
    <insert id="create" parameterType="User">
        INSERT INTO USERS (USERID, PASSWORD, EMAIL, PHONENUMBER, NICKNAME) VALUES (#{userId}, #{password}, #{email}, #{phoneNumber}, #{nickName})
    </insert>
    <select id="selectById" resultType="User" parameterType="String">
        SELECT * FROM USERS WHERE USERID = #{userId}
    </select>
</mapper>