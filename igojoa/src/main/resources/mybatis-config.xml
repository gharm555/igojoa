<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-config.dtd">

<!-- MyBatis3 프레임워크 설정 -->
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	<typeAliases>
		<typeAlias alias="User" type="com.itwill.igojoa.entity.User" />
		<typeAlias alias="Place" type="com.itwill.igojoa.entity.Place" />
		<typeAlias alias="PScores" type="com.itwill.igojoa.entity.PScores" />
		<typeAlias alias="Reviews" type="com.itwill.igojoa.entity.Reviews" />
		<typeAlias alias="PVerified" type="com.itwill.igojoa.entity.PVerified"/>
		<typeAlias alias="Point" type="com.itwill.igojoa.entity.Point"/>
		<typeAlias alias="PointsLog" type="com.itwill.igojoa.entity.PointsLog"/>
	</typeAliases>

</configuration>